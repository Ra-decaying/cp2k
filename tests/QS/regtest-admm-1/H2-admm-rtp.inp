&FORCE_EVAL
  METHOD QUICKSTEP
  &DFT
    &REAL_TIME_PROPAGATION
       MAX_ITER 5
       MAT_EXP TAYLOR
       EPS_ITER 1.0E-9
       INITIAL_WFN SCF_WFN
    &END
    BASIS_SET_FILE_NAME BASIS_SET
    BASIS_SET_FILE_NAME BASIS_ADMM
    POTENTIAL_FILE_NAME POTENTIAL
    &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD BASIS_PROJECTION
      ADMM_PURIFICATION_METHOD NONE
    &END
    &MGRID
      CUTOFF 100
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-10
    &END QS
    &SCF
      MAX_SCF 60
      EPS_SCF 1.0E-3
      SCF_GUESS ATOMIC
      &OUTER_SCF
      EPS_SCF 1.0E-3
      &END
      &OT ON
        ROTATION
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL NONE
      &END XC_FUNCTIONAL
      &HF
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 2.5
        &END
        &SCREENING
          EPS_SCHWARZ 1.0E-14
        &END
      &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 6.0 6.0 6.0
    &END CELL
    &COORD
   H  6.000000   4.5000000   6.000000
   H  6.000000   1.5000000    6.000000
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PBE
      BASIS_SET AUX_FIT FIT3
      POTENTIAL GTH-PBE-q1
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2-admm-rtp
 RUN_TYPE RT_PROPAGATION
  PRINT_LEVEL LOW
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 2
    TIMESTEP [au_t] 0.25
    TEMPERATURE 300.0
  &END MD
    &PRINT
       &TRAJECTORY
          &EACH
             MD 1
          &END
       &END
    &END
&END MOTION
